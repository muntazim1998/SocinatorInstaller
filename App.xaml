<Application x:Class="SocinatorInstaller.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SocinatorInstaller"
             StartupUri="DriverWindow.xaml">
    <Application.Resources>

        <FontFamily x:Key="RubikBold">pack://application:,,,/Rubik/Rubik-Bold.ttf#Rubik</FontFamily>
        <FontFamily x:Key="RubikLight">pack://application:,,,/Rubik/Rubik-Light.ttf#Rubik</FontFamily>
        <FontFamily x:Key="RubikRegular">pack://application:,,,/Rubik/Rubik-Regular.ttf#Rubik</FontFamily>
         <FontFamily x:Key="RubikMedium">pack://application:,,,/Rubik/Rubik-Medium.ttf#Rubik</FontFamily>
        <FontFamily x:Key="RubikSemiBold">pack://application:,,,/Rubik/Rubik-SemiBold.ttf#Rubik</FontFamily>
    </Application.Resources>
</Application>
