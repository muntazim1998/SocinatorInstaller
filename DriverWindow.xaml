<Window x:Class="SocinatorInstaller.DriverWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SocinatorInstaller"
        mc:Ignorable="d"
        WindowStyle="None"
        FontFamily="{StaticResource RubikRegular}"
        WindowStartupLocation="CenterScreen"
        Height="550" Width="850"
        ResizeMode="NoResize" AllowsTransparency="True"
        SizeToContent="WidthAndHeight"
        Title="Socinator Setup"  Icon="/favicon.ico" >
    <Grid>
        <UserControl Content="{Binding SelectedUserControl,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>
    </Grid>
</Window>
